{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
      "jQuery": true,
      "define": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-debugger": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      0,
      "never"
    ],
    "strict": [
        2,
        "function"
    ],
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}